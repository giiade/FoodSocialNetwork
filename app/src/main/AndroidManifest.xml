<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="se.FSN.foodsocialnetwork" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="se.FSN.foodsocialnetwork.Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="se.FSN.foodsocialnetwork.Splash"
            android:label="@string/title_activity_splash" />
        <activity
            android:name="se.FSN.foodsocialnetwork.Login"
            android:label="@string/title_activity_login" />
        <activity
            android:name="se.FSN.foodsocialnetwork.PostRecipe"
            android:label="@string/title_activity_post_recipe" >
        </activity>
    </application>

</manifest>
